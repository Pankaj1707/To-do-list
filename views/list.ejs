<%- include("header") %> 

    <div class="box" id="heading">
        <h1><%= typeOfWork %> </h1> 
    </div>



    <div class="box ">
        <%  for(var i=0; i<newItems.length; i++) {   %>

            <div class="item">
               
               <input type="checkbox">
               <p><%= newItems[i]%> </p>
           </div>
           <% }  %>
       
        <form class="item" action="/" method="post">
            <input name = "newItem" type="text" placeholder="New item" autocomplete="off">
            <button type="submit" name="list" value=<%=typeOfWork %> >+</button>
        </form>
        
        
    </div>
    
<%- include("footer") %> 
